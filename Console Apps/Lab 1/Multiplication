namespace _5
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Enter a number: ");
            int num = Convert.ToInt32(Console.ReadLine());

            for (int i = 1; i <= 12; i++) 
            {
                Console.WriteLine($"{num} * {i} = {num * i}");
            }
            Console.ReadLine();
        }
    }
}
